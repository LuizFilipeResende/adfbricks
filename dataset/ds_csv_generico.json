{
	"name": "ds_csv_generico",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_datalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ContainerName": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			},
			"FileSeparator": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().ContainerName",
					"type": "Expression"
				}
			},
			"columnDelimiter": {
				"value": "@dataset().FileSeparator",
				"type": "Expression"
			},
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}